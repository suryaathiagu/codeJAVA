import java.util.*;
class fibo
{
    public static void main(String... args)
    {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0,a=0,b=1;
        System.out.println(a);
        System.out.println(b);
        for(int i=1;i<n;i++)
        {
            sum+=i;
            System.out.println(sum);
        }
    }
}
